    "morgan": "^1.9.1",
    "nedb": "^1.8.0",
    "node-xlsx": "^0.12.1",
    "pug": "^2.0.3",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
